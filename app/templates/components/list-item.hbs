<div class="card hoverable" data-card-id={{list.id}}>
  <div class="card-image truncate">
    {{!-- <img src="https://placeholdit.imgix.net/~text?txtsize=33&txt=350%C3%97150&w=350&h=150"> --}}
    {{lazy-image url='https://placeholdit.imgix.net/~text?txtsize=33&txt=350%C3%97150&w=350&h=150' errorText='Something went wrong.'}}
    <span class="card-title">{{list.name}}</span>
  </div>
  <div class="card-action">
  {{#link-to "list" list class="waves-effect waves-light"}}
    View
  {{/link-to}}
  </div>
</div>
{{yield}}
